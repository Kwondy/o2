				}
			}
			
			function check_blank(checkStr)
			{
				var str_len = 0
				for (var i=0;i < checkStr.length; i++)
				{
						var str_temp = checkStr;
						if (str_temp.substring(i,i+1) != ' ')
								str_len=str_len+1;
								if (str_len != 0)
								{
									return (true);
									break;
								}
				}
				return (false);
			}
			
		
