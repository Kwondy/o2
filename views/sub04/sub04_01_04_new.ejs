						document.formname.password.focus();
					}
					else if (check_blank(document.formname.title.value) == false)
					{
						alert ("제목을 입력해 주세요.");
						document.formname.title.focus();
					}
					//jjs 20130411
					else if (check_blank(document.formname.content.value) == false)
					{
